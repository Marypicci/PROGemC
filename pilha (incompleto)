#include <stdio.h>

typedef struct cel {
int chave;
struct cel *prox;
} celula;

void empilha_enc_C(celula y, celula *t)
{
celula *nova;
nova = (celula *) malloc(sizeof (celula));
nova->chave = y;
nova->prox = t -> prox;
t->prox = nova;
}

int desempilhar_enc_C(celula *t)
{
int x;
celula *p;


if(t->prox != NULL){
    p = t->prox;
    x = p->conteudo;
    t->prox = p->prox;
    free(p);
    return x;
}
else{
    printf ("Pilha vazia!\n");
    return INT_MIN;
}
}

int main(){
    int n;
    celula *t;
    t = (celula *) malloc(sizeof (celula));
    t->prox = NULL;

    printf("Me informe o número que será posto na pilha: \n");
    scanf("%d", &n);

    
}
